<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="dame.pages.Profil.VueProfil">


	<HBox alignment="center">
		<VBox>	
			<Text text="avatar"/>
			<TextField fx:id="avatarText"/>
		</VBox>
		<VBox >
			<Text text="Nom:" />
			<TextField fx:id="nomText"/>
			<Text text="Age:"/>
			<TextField fx:id="ageText"/>
			<Text text="Description:"/>
			<TextField fx:id="descriptionText"/>
			<Text text="Statistique:"/>
			<TextField fx:id="statistiqueText"/>
			<Button fx:id="buttonSauvegarder" text="Sauvegarder"/>
		</VBox>
	
	</HBox>
</VBox>